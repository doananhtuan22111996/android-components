<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="App.Widget.Material3.Button.TextButton" parent="Widget.Material3.Button.TextButton" />

    <style name="App.Widget.Material3.Button.OutlinedButton" parent="Widget.Material3.Button.OutlinedButton" />

    <style name="App.Widget.Material3.Button.OutlinedButton.Icon" parent="Widget.Material3.Button.OutlinedButton.Icon" />

    <style name="App.Widget.Material3.Button.TextButton.Icon" parent="Widget.Material3.Button.TextButton.Icon" />

    <style name="App.Widget.Material3.Button.IconButton.Outlined" parent="Widget.Material3.Button.IconButton.Outlined" />

    <style name="App.Widget.Material3.Button.ElevatedButton" parent="Widget.Material3.Button.ElevatedButton" />

    <style name="App.Widget.Material3.Button.ElevatedButton.Icon" parent="Widget.Material3.Button.ElevatedButton.Icon" />

    <style name="App.Widget.Material3.Button.TonalButton" parent="Widget.Material3.Button.TonalButton" />

    <style name="App.Widget.Material3.Button.TonalButton.Icon" parent="Widget.Material3.Button.TonalButton.Icon" />

    <style name="App.Widget.Material3.Button.OutlinedButton.IconOnly" parent="Widget.Material3.Button.OutlinedButton">
        <item name="iconPadding">0dp</item>
        <item name="android:insetTop">0dp</item>
        <item name="android:insetBottom">0dp</item>
        <item name="android:paddingLeft">12dp</item>
        <item name="android:paddingRight">12dp</item>
        <item name="android:minWidth">48dp</item>
        <item name="android:minHeight">48dp</item>
    </style>

    <style name="App.Widget.Material3.FloatingActionButton.Primary" parent="Widget.Material3.FloatingActionButton.Primary">
        <item name="shapeAppearance">
            @style/App.ShapeAppearance.Material3.FloatingActionButton.Primary.Rounded
        </item>
    </style>

    <style name="App.ShapeAppearance.Material3.FloatingActionButton.Primary.Rounded" parent="ShapeAppearance.Material3.LargeComponent">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">28dp</item>
    </style>

    <style name="App.Widget.Material3.FloatingActionButton.Secondary" parent="Widget.Material3.FloatingActionButton.Secondary">
        <item name="materialThemeOverlay">
            @style/App.ThemeOverlay.Material3.FloatingActionButton.Secondary
        </item>
    </style>

    <style name="App.ShapeAppearance.Material3.FloatingActionButton.Secondary.Cut" parent="ShapeAppearance.Material3.LargeComponent">
        <item name="cornerFamily">cut</item>
        <item name="cornerSize">28dp</item>
    </style>

    <style name="App.ThemeOverlay.Material3.FloatingActionButton.Secondary" parent="ThemeOverlay.Material3.FloatingActionButton.Secondary">
        <item name="shapeAppearanceCornerLarge">
            @style/App.ShapeAppearance.Material3.FloatingActionButton.Secondary.Cut
        </item>
    </style>

</resources>